{
    "name": "aybarsm/laravel-os",
    "description": "A service provider for Laravel and Laravel Zero, facilitating OS-level interactions and management for applications.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "aybarsm",
        "laravel",
        "operating-syetem",
        "linux",
        "windows",
        "mac",
        "macOS",
        "Darwin",
        "BSD",
        "Debian",
        "Ubuntu",
        "Oracle Linux",
        "AlmaLinux",
        "Rocky Linux",
        "Alpine"
    ],
    "homepage": "https://github.com/aybarsm/laravel-os",
    "authors": [
        {
            "name": "Murat Aybars",
            "email": "hello@aybarsm.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "aybarsm/laravel-extended-support": "dev-main"
    },
    "require-dev": {
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Aybarsm\\Laravel\\Os\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aybarsm\\Laravel\\Os\\OsServiceProvider"
            ],
            "aliases": {
                "Os": "Aybarsm\\Laravel\\Os\\Facades\\Os"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
